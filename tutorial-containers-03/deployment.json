                "createOptions": "\"HostConfig\": {\"Privileged\": true,\"PortBindings\": {\"80/tcp\": [{\"HostPort\": \"80\"}]},\"Devices\": [{\"PathOnHost\": \"/dev/i2c-1\",\"PathInContainer\": \"/dev/i2c-1\",\"CgroupPermissions\": \"mrw\"}]}"
